   <children>
      <TextField id="txtAlias" fx:id="txtAlias" layoutX="74.0" layoutY="124.0" onKeyPressed="#onKeyPressed" prefHeight="43.0" prefWidth="230.0" promptText="Enter alias...">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button fx:id="btnLogin" layoutX="74.0" layoutY="267.0" mnemonicParsing="false" onAction="#onLoginClicked" prefHeight="40.0" prefWidth="230.0" style="-fx-background-color: rgb(19, 155, 240); -fx-font-weight: 500;" text="LOG IN" textAlignment="JUSTIFY" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Text fill="#0b4dba" layoutX="74.0" layoutY="86.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: 700;" text="Log In" wrappingWidth="132.7294921875">
         <font>
            <Font size="21.0" />
         </font>
      </Text>
      <Label fx:id="lblErrAlias" layoutX="74.0" layoutY="167.0" prefHeight="24.0" prefWidth="230.0" textFill="RED" />
      <Label fx:id="lblErrPassword" layoutX="74.0" layoutY="240.0" prefHeight="24.0" prefWidth="230.0" textFill="RED" />
      <PasswordField fx:id="txtPassword" layoutX="74.0" layoutY="197.0" onKeyPressed="#onKeyPressed" prefHeight="43.0" prefWidth="230.0" promptText="Enter Password...">
